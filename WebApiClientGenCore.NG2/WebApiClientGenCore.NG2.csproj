<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <PackageId>Fonlow.WebApiClientGenCore.NG2</PackageId>
    <Authors>Zijian Huang</Authors>
    <Description>Plugin for WebApiClientGen 3.0+ to generate client API codes with Angular 6+.</Description>
    <PackageReleaseNotes>* If service function return type is void, the client API return is Observable&lt;HttpResponse&lt;string&gt;&gt; so client codes could handle response details.</PackageReleaseNotes>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageProjectUrl>https://github.com/zijianhuang/webapiclientgen</PackageProjectUrl>
    <Copyright>Copyright Â© Zijian Huang 2015-2020</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <AssemblyName>Fonlow.WebApiClientGenCore.NG2</AssemblyName>
    <AssemblyVersion>1.2.1.0</AssemblyVersion>
    <FileVersion>1.2.1.0</FileVersion>
    <Version>1.2.1</Version>
    <PackageTags>Angular</PackageTags>
  </PropertyGroup>

  <Import Project="..\PluginNG2Shared\PluginNG2Shared.projitems" Label="Shared" />

  <ItemGroup>
    <ProjectReference Include="..\Fonlow.DocCommentCore\Fonlow.DocCommentCore.csproj" />
    <ProjectReference Include="..\Fonlow.Poco2TsCore\Fonlow.Poco2TsCore.csproj" />
    <ProjectReference Include="..\Fonlow.TypeScriptCodeDomCore\Fonlow.TypeScriptCodeDomCore.csproj" />
    <ProjectReference Include="..\Fonlow.Web.MetaCore\Fonlow.Web.MetaCore.csproj" />
    <ProjectReference Include="..\WebApiClientGenCore.Abstract\WebApiClientGenCore.Abstract.csproj" />
  </ItemGroup>

</Project>
