    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Version>1.3</Version>
    <Description>Basic functions of accessing XML Doc Comments.</Description>
    <PackageReleaseNotes>For .NET Core 3.1</PackageReleaseNotes>
    <Copyright>Copyright Â© Zijian Huang 2015-2020</Copyright>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/zijianhuang/webapiclientgen</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>1.3.0.0</AssemblyVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <NeutralLanguage>en</NeutralLanguage>
    <FileVersion>1.3.0.0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <NoWarn>1701;1702;1705;1591</NoWarn>
  </PropertyGroup>

  <Import Project="..\Fonlow.DocCommentShared\Fonlow.DocCommentShared.projitems" Label="Shared" />

