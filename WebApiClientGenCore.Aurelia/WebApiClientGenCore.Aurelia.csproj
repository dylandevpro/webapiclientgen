<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageId>Fonlow.WebApiClientGenCore.Aurelia</PackageId>
    <Authors>Zijian Huang</Authors>
    <Description>Generate client API codes for Aurelia apps.</Description>
    <Copyright>Copyright Â© Zijian Huang 2015-2019</Copyright>
    <PackageTags>aurelia TypeScript</PackageTags>
    <PackageReleaseNotes>Generate client API codes for Aurelia apps.</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/zijianhuang/webapiclientgen</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <AssemblyName>Fonlow.WebApiClientGenCore.Aurelia</AssemblyName>
  </PropertyGroup>

  <Import Project="..\PluginAureliaShared\PluginAureliaShared.projitems" Label="Shared" />

  <ItemGroup>
    <ProjectReference Include="..\Fonlow.DocCommentCore\Fonlow.DocCommentCore.csproj" />
    <ProjectReference Include="..\Fonlow.Poco2TsCore\Fonlow.Poco2TsCore.csproj" />
    <ProjectReference Include="..\Fonlow.TypeScriptCodeDomCore\Fonlow.TypeScriptCodeDomCore.csproj" />
    <ProjectReference Include="..\Fonlow.Web.MetaCore\Fonlow.Web.MetaCore.csproj" />
    <ProjectReference Include="..\WebApiClientGenCore.Abstract\WebApiClientGenCore.Abstract.csproj" />
  </ItemGroup>

</Project>
