<?xml version="1.0"?>
<package >
  <metadata>
    <id>Fonlow.Poco2Ts</id>
    <version>$version$</version>
    <title>Generate data models in TypsScript from POCO classes</title>
    <authors>Zijian Huang</authors>
    <owners>Zijian Huang</owners>
    <licenseUrl>https://webapiclientgen.codeplex.com/license</licenseUrl>
    <projectUrl>https://webapiclientgen.codeplex.com</projectUrl>
    <iconUrl>https://webapiclientgen.codeplex.com</iconUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>Generate data model interfaces in TypsScript from POCO classes. This has been used in POCO2TS.exe and Strongly Typed Client API for Web API.</description>
    <releaseNotes>
      translate System.ComponentModel.DataAnnotations attributes into doc comment.
      [System.ComponentModel.DataAnnotations.StringLength(int.MaxValue, MinimumLength = 8)]
      [System.ComponentModel.DataAnnotations.Range(1, int.MaxValue)]
      [System.ComponentModel.DataAnnotations.MaxLength(20)]
      [System.ComponentModel.DataAnnotations.MinLength(1)]
      [System.ComponentModel.DataAnnotations.RegularExpression(@"/(?=.[a-z])(?=.[A-Z])(?=.*[0-9])/")]
    </releaseNotes>
    <copyright>Copyright Â© Fonlow 2015-2020</copyright>
    <tags>TypeScript CodeDOM API Web Client webapi restful aspnet aspnetwebapi</tags>
    <dependencies>
      <group>
        <dependency id="Fonlow.TypeScriptCodeDOM" version="2.3.0" />
      </group>
    </dependencies>  
  </metadata>
  <files>
    <file src="bin\Release\Fonlow.DocComment.dll" target="lib\net462" />
  </files>
</package>